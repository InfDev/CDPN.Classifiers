{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ClassifiersDbProvider": "sqlite", // sqlite, sqlserver, mysql, postgresql
  "ClassifiersConnectionStrings": {
    "sqlite": "Data Source=CdpnClassifiers.db",
    "sqlserver": "Server=(localdb)\\mssqllocaldb;Database=CdpnClassifiers;Trusted_Connection=true;MultipleActiveResultSets=true",
    "mysql": "Server=127.0.0.1;Port=3306;Database=CdpnClassifiers;Uid=myUsername;Pwd=myPassword;",
    "postgresql": "Server=127.0.0.1;Port=5432;Database=CdpnClassifiers;Integrated Security=true;"
  },
  "RetryMigrations": false,
  "SeqServerUrl": ""
}
